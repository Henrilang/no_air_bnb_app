<div class="titlebooking">
  <h1>Create a booking</h1><br>
    <% if @planet.user_id != current_user.id %>
      <div>
        <%= simple_form_for [@planet, @booking] do |f| %>
          <%= f.input :start_date, as: :date, html5: true %>
          <%= f.input :end_date, as: :date, html5: true  %>
          <%= f.submit class: 'btn btn-block', value: 'Create' %>
        <% end %>
      </div>
    <% end %>
</div>
